# Environment Variables Template

# Google AI API Key (REQUIRED)
GOOGLE_API_KEY=your_api_key_here

# Logging Level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Dry Run Mode (true/false) - Test without making API calls
DRY_RUN=false

# Maximum Cost Limit (USD)
MAX_COST_LIMIT=50.00

# Flask Secret Key (change in production)
FLASK_SECRET_KEY=change-this-in-production-to-random-string

# Flask Debug Mode (true/false)
FLASK_DEBUG=true
